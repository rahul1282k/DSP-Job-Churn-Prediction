{
  "id": null,
  "uid": "ingestion-dashboard",
  "title": "Ingestion Monitoring",
  "schemaVersion": 36,
  "version": 1,
  "timezone": "browser",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Invalid rows percentage",
      "type": "timeseries",
      "datasource": "Postgres",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 20 },
              { "color": "red", "value": 40 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "refId": "A",
          "datasource": "Postgres",
          "format": "time_series",
          "rawSql": "SELECT\n  created_at AS \"time\",\n  (invalid_rows::float / NULLIF(total_rows,0)) * 100 AS invalid_pct\nFROM data_quality_stats\nWHERE $__timeFilter(created_at)\nORDER BY created_at;"
        }
      ]
    },
    {
      "id": 2,
      "title": "Valid vs invalid rows per ingestion",
      "type": "timeseries",
      "datasource": "Postgres",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true },
        "tooltip": { "mode": "single" },
        "stacking": { "mode": "normal", "group": "A" }
      },
      "targets": [
        {
          "refId": "A",
          "datasource": "Postgres",
          "format": "time_series",
          "rawSql": "SELECT\n  created_at AS \"time\",\n  valid_rows,\n  invalid_rows\nFROM data_quality_stats\nWHERE $__timeFilter(created_at)\nORDER BY created_at;"
        }
      ]
    },
    {
      "id": 3,
      "title": "Criticality counts over time",
      "type": "timeseries",
      "datasource": "Postgres",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true },
        "tooltip": { "mode": "single" },
        "stacking": { "mode": "normal", "group": "crit" }
      },
      "targets": [
        {
          "refId": "A",
          "datasource": "Postgres",
          "format": "time_series",
          "rawSql": "SELECT\n  date_trunc('minute', created_at) AS \"time\",\n  SUM(CASE WHEN criticality = 'low' THEN 1 ELSE 0 END) AS low,\n  SUM(CASE WHEN criticality = 'medium' THEN 1 ELSE 0 END) AS medium,\n  SUM(CASE WHEN criticality = 'high' THEN 1 ELSE 0 END) AS high\nFROM data_quality_stats\nWHERE $__timeFilter(created_at)\nGROUP BY 1\nORDER BY 1;"
        }
      ]
    },
    {
      "id": 4,
      "title": "Total data quality issues per ingestion",
      "type": "timeseries",
      "datasource": "Postgres",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": false },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "refId": "A",
          "datasource": "Postgres",
          "format": "time_series",
          "rawSql": "SELECT\n  s.created_at AS \"time\",\n  COALESCE(SUM((s.issues ->> key)::int), 0) AS total_issues\nFROM data_quality_stats s\nLEFT JOIN LATERAL jsonb_object_keys(s.issues) AS key ON TRUE\nWHERE $__timeFilter(s.created_at)\nGROUP BY s.created_at\nORDER BY s.created_at;"
        }
      ]
    },
    {
      "id": 5,
      "title": "Issues by type (histogram)",
      "type": "barchart",
      "datasource": "Postgres",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true },
        "tooltip": { "mode": "single" },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "refId": "A",
          "datasource": "Postgres",
          "format": "table",
          "rawSql": "SELECT\n  key AS issue_type,\n  SUM((s.issues ->> key)::int) AS issue_count\nFROM data_quality_stats s,\n     LATERAL jsonb_object_keys(s.issues) AS key\nWHERE $__timeFilter(s.created_at)\nGROUP BY key\nORDER BY issue_count DESC;"
        }
      ]
    },
    {
      "id": 6,
      "title": "Rows ingested per minute",
      "type": "timeseries",
      "datasource": "Postgres",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": false },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "refId": "A",
          "datasource": "Postgres",
          "format": "time_series",
          "rawSql": "SELECT\n  date_trunc('minute', created_at) AS \"time\",\n  SUM(total_rows) AS rows_ingested\nFROM data_quality_stats\nWHERE $__timeFilter(created_at)\nGROUP BY 1\nORDER BY 1;"
        }
      ]
    }
  ],
  "time": {
    "from": "now-2h",
    "to": "now"
  },
  "templating": {
    "list": []
  }
}
