{
  "expectation_suite_name": "ingestion_suite",
  "meta": {
    "great_expectations_version": "0.17.21"
  },
  "ge_cloud_id": null,
  "data_asset_type": "Dataset",
  "expectations": [
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "gender" }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "city_development_index" }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "training_hours" }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "relevent_experience" }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "target" }
    },

    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "city_development_index",
        "min_value": 0.0,
        "max_value": 1.0
      }
    },

    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "training_hours",
        "min_value": 0,
        "max_value": 1000
      }
    },

    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "gender",
        "value_set": ["Male", "Female", "Other"]
      }
    },

    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "relevent_experience",
        "value_set": ["Has relevent experience", "No relevent experience"]
      }
    },

    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "target",
        "value_set": [0, 1]
      }
    }
  ]
}
